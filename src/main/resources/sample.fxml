<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>

<GridPane alignment="center" hgap="10" vgap="10" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller">
    <columnConstraints>
        <ColumnConstraints />
    </columnConstraints>
    <rowConstraints>
        <RowConstraints />
    </rowConstraints>
    <children>
        <SplitPane prefHeight="423.0" prefWidth="645.0">
            <items>
                <AnchorPane fx:id="leftPanel" minHeight="0.0" minWidth="0.0" prefHeight="160.0" prefWidth="100.0">
               <children>
                  <Button fx:id="cleanButton" layoutX="12.0" layoutY="378.0" mnemonicParsing="false" prefHeight="29.0" prefWidth="619.0" text="Clean" />
                  <CheckBox fx:id="chromeCacheCB" layoutX="45.0" layoutY="80.0" mnemonicParsing="false" text="Chrome Cache" />
                  <CheckBox fx:id="chromeHistoryCB" layoutX="45.0" layoutY="105.0" mnemonicParsing="false" text="Chrome History" />
                  <CheckBox fx:id="chromeCookiesCB" layoutX="45.0" layoutY="130.0" mnemonicParsing="false" text="Chrome Cookies" />
                  <CheckBox fx:id="googleChromeCB" layoutX="45.0" layoutY="43.0" mnemonicParsing="false" onMouseClicked="#onClickMethodChrome" text="Google Chrome" />
                  <CheckBox fx:id="systemCB" layoutX="276.0" layoutY="43.0" mnemonicParsing="false" onMouseClicked="#onClickMethodSystem" text="System" />
                  <CheckBox fx:id="recycleBinCB" layoutX="276.0" layoutY="80.0" mnemonicParsing="false" text="Recycle Bin" />
                  <CheckBox fx:id="tempFilesCB" layoutX="276.0" layoutY="105.0" mnemonicParsing="false" text="Temporary Files" />
               </children></AnchorPane>
            </items>
        </SplitPane>
    </children>
</GridPane>
